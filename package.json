{
  "name": "liasaldana",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.tsx",
    "build:all": "tsc && npm run build:client && npm run build:server",
    "generate": "vite build --outDir dist/static && npm run build:all && node prerender",
    "dev:server": "node server"
  },
  "dependencies": {
    "@studio-freight/lenis": "^1.0.33",
    "@studio-freight/react-lenis": "^0.0.40",
    "@unreal/pan": "^0.0.23",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0"
  },
  "devDependencies": {
    "@studio-freight/hamo": "^0.6.24",
    "@svgr/rollup": "^8.1.0",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "express": "^4.18.2",
    "sass": "^1.69.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
